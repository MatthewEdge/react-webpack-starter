{
  "name": "react-webpack-starter",
  "version": "0.0.1",
  "description": "",
  "author": "Matthew Edge",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel wds devServer",
    "test": "mocha --require babel-register --require test/testHelpers.js \"src/**/*.spec.js\" --recursive",
    "build": "webpack --progress --colors --env develop",
